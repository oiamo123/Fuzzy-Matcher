            "name": "Nickname_Ben_to_Benjamin",
            "description": "Should find Benjamin Anderson when searching for Ben Anderson",
            "query": {
                "firstname": "Ben",
                "surname": "Anderson",
                "birthdate": "1989-09-14",
                "event_start_utc": "2999-08-20T18:00:00Z"
            },
            "expected": {
                "should_find": true,
                "min_matches": 1,
                "expected_matches": [
                    {
                        "member_id": 9,
                        "min_score": 0.6,
                        "max_score": 1.0,
                        "note": "Ben should fuzzy match to Benjamin"
                    }
                ]
            }
        }, 
